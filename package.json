{
  "name": "steamcommunity-enhanced",
  "version": "0.0.1",
  "description": "A browser plugin to enhance the SteamCommunity",
  "main": "index.js",
  "scripts": {
    "start": "concurrently --kill-others \"tsc -w\" \"web-ext run --source-dir=./dist --keep-profile-changes --firefox-profile=steamcommunity_enhanced_dev\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Compufreak345/steamcommunity-enhanced.git"
  },
  "keywords": [
    "typescript",
    "steam",
    "steamcommunity",
    "webextension",
    "chrome",
    "firefox"
  ],
  "author": "Compufreak345 (Christoph Sonntag)",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/Compufreak345/steamcommunity-enhanced/issues"
  },
  "homepage": "https://github.com/Compufreak345/steamcommunity-enhanced#readme",
  "dependencies": {
    "jquery": "^3.3.1",
    "nodemon": "^1.14.12"
  },
  "devDependencies": {
    "@types/jquery": "^3.3.0",
    "concurrently": "^3.5.1"
  }
}
